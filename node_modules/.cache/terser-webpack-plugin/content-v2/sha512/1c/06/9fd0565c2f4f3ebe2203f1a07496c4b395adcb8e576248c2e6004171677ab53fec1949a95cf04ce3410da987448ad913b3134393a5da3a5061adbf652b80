 schoolId = \\\"northwestern\\\";\\n\\tconst clubId = useParams();\\n\\tconst { currentUser, isExec, inClub } = useAuth();\\n\\n\\treturn (\\n\\t\\t<ClubDetails\\n\\t\\t\\t{...props}\\n\\t\\t\\tschoolId={schoolId}\\n\\t\\t\\tclubId={clubId.clubId}\\n\\t\\t\\tcurrentUser={currentUser}\\n\\t\\t\\tisExec={isExec}\\n\\t\\t\\tinClub={inClub}\\n\\t\\t/>\\n\\t);\\n}\\n\",\"import * as React from \\\"react\\\";\\nimport IconButton from \\\"@mui/material/IconButton\\\";\\nimport Menu from \\\"@mui/material/Menu\\\";\\nimport MenuItem from \\\"@mui/material/MenuItem\\\";\\nimport MoreVertIcon from \\\"@mui/icons-material/MoreVert\\\";\\nimport {\\n\\tpromoteUsers,\\n\\tremoveUsers,\\n} from \\\"../../../../../../Utilities/firebase\\\";\\n\\nconst options = [\\\"Promote\\\", \\\"Remove\\\"];\\n\\nconst ITEM_HEIGHT = 48;\\n\\nconst ClubExecMenu = (props) => {\\n\\tconst { selectedUIDs, clubRef, handleRemoveRows, handleAddExec } = props;\\n\\tconst [anchorEl, setAnchorEl] = React.useState(null);\\n\\tconst open = Boolean(anchorEl);\\n\\n\\tconst handleClick = (event) => {\\n\\t\\tsetAnchorEl(event.currentTarget);\\n\\t};\\n\\tconst handleClose = (option) => {\\n\\t\\tsetAnchorEl(null);\\n\\t\\tif (option === \\\"Promote\\\") {\\n\\t\\t\\thandlePromote();\\n\\t\\t\\thandleAddExec(selectedUIDs);\\n\\t\\t\\thandleRemoveRows([]);\\n\\t\\t} else if (option == \\\"Remove\\\") {\\n\\t\\t\\thandleRemove();\\n\\t\\t\\thandleRemoveRows(selectedUIDs);\\n\\t\\t}\\n\\t};\\n\\n\\tconst handlePromote = () => {\\n\\t\\tpromoteUsers(selectedUIDs, clubRef);\\n\\t};\\n\\tconst handleRemove = () => {\\n\\t\\tremoveUsers(selectedUIDs, clubRef);\\n\\t};\\n\\n\\treturn (\\n\\t\\t<div>\\n\\t\\t\\t<IconButton\\n\\t\\t\\t\\taria-label='more'\\n\\t\\t\\t\\tid='long-button'\\n\\t\\t\\t\\taria-controls={open ? \\\"long-menu\\\" : undefined}\\n\\t\\t\\t\\taria-expanded={open ? \\\"true\\\" : undefined}\\n\\t\\t\\t\\taria-haspopup='true'\\n\\t\\t\\t\\tonClick={handleClick}>\\n\\t\\t\\t\\t<MoreVertIcon id='but